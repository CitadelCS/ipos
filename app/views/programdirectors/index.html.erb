

<h1>Professors</h1>

<table>
  <thead>
    <tr>
     <thead>
    <tr>
      <th>Name</th>
     
      <th colspan="3"></th>
    </tr>
  </thead>
    </tr>
  </thead>

  <tbody>
    <% @professor.each do |professor| %>
      <tr>
        <td><%= professor.Name %></td>
        <td><%= link_to 'Show', professor %></td>
        <td><%= link_to 'Edit', edit_professor_path(professor) %></td>
        <td><%= link_to 'Destroy', professor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professor', new_professor_path %>


<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cwid</th>
      <th>Concentration</th>
      <th>Advisor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student.each do |student| %>
      <tr>
        <td><%= student.Name %></td>
        <td><%= student.CWID %></td>
        <td><%= student.Concentration %></td>
        <td><%= student.Advisor %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>

<h1>Courses</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Section</th>
      <th>Number</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 <tbody>
  <% @course.each do |course| %>
   <tr>
       <td><%=course.prefix%></td>
       <td><%= link_to course.name, course %></td>
       <td><%=course.section%></td>
       <td><%=course.number%></td>
      <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</tbody>
</table>
<%= link_to "New Course", new_course_path %>